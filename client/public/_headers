# Handle CSS files with proper MIME types
*.css
  Content-Type: text/css
  Cache-Control: public, max-age=31536000, immutable

# Handle JavaScript files with proper MIME types
*.js
  Content-Type: application/javascript
  Cache-Control: public, max-age=31536000, immutable

# Handle module JavaScript files
*.mjs
  Content-Type: application/javascript
  Cache-Control: public, max-age=31536000, immutable

# Explicit handling for the specific JS file
/index-CqFGXhra.js
  Content-Type: application/javascript
  Cache-Control: public, max-age=31536000, immutable

# Explicit handling for the specific CSS file
/index-UDW46HgK.css
  Content-Type: text/css
  Cache-Control: public, max-age=31536000, immutable
